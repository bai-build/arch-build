	install -m644 "$srcdir/$pkgname/Content/y.png" "$pkgdir/usr/share/$pkgname/Content"
	install -m644 "$srcdir/$pkgname/Content/zap.wav" "$pkgdir/usr/share/$pkgname/Content"
	install -m644 "$srcdir/$pkgname/Content/z.png" "$pkgdir/usr/share/$pkgname/Content"
} 
