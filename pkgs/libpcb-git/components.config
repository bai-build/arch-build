CONFIG_SAH_LIB_PCB=y
CONFIG_PCB_OPEN_SSL_SUPPORT=y
CONFIG_PCB_ALLOW_ANONYMOUS_TCP=y
CONFIG_PCB_DEFAULT_PLUGIN_PATH="/usr/lib/pcb"
CONFIG_PCB_ACL_USERMNGT=y

CONFIG_SAH_LIB_USERMNGT_USERS_COUNT=16
CONFIG_SAH_LIB_USERMNGT_GROUPS_COUNT=16

CONFIG_DEFAULT_CIPHER_SUITE_LIST="EDH+CAMELLIA:EDH+aRSA:EECDH+aRSA+AESGCM:EECDH+aRSA+SHA384:EECDH+aRSA+SHA256:EECDH:+CAMELLIA256:+AES256:+CAMELLIA128:+AES128:+SSLv3:!aNULL:!eNULL:!LOW:!3DES:!MD5:!EXP:!PSK:!DSS:!RC4:!SEED:CAMELLIA256-SHA:AES256-SHA:CAMELLIA128-SHA:AES128-SHA"
