pkgname=qpi-libs
pkgver=6.6.6
pkgrel=1
pkgdesc="Qt ${pkgver} libs for the ${_piver}"
# I don't know how to set this correct for cross architecture builds
arch=("any")
url="http://www.qt.io"
license=("LGPL3")
source=()
sha256sums=()
options=('!strip')

package() {
    mv ${startdir}/topkg/* ${pkgdir}
}
